{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"module": "esnext",
		"target": "esnext",
		"lib": [
			"esnext",
			"esnext.array",
			"esnext.asynciterable",
			"esnext.intl",
			"esnext.symbol",
			"dom"
		],
		"esModuleInterop": true,
		"baseUrl": ".",
		"paths": {
			"~/*": [
				"./*"
			],
			"@/*": [
				"./*"
			]
		},
		"types": [
			"@types/node",
			"@nuxt/vue-app"
		]
	}
}
