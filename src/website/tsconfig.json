{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"module": "ESNext",
		"target": "es5",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"allowJs": true,
		"noEmit": true,
		"incremental": false,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"forceConsistentCasingInFileNames": false
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx"
	],
	"exclude": [
		"node_modules"
	]
}
