		"build": "tsc",
		"postbuild": "npm run copy-locales",
		"copy-locales": "cpy locales/**/*.* dist/",
		"test": "jest --passWithNoTests",
		"test:ci": "jest --no-stack-trace --verbose --passWithNoTests",
		"lint": "prettier --check . && eslint src --ext .ts",
		"format": "prettier --write . && eslint src --ext ts --fix",
		"start": "env-cmd env-cmd node --enable-source-maps --es-module-specifier-resolution=node dist/index.js",
		"dev": "env-cmd node --enable-source-maps --es-module-specifier-resolution=node dist/index.js",
		"deploy:commands": "env-cmd node --enable-source-maps --es-module-specifier-resolution=node dist/deploy.js",
