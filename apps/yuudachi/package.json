{
	"name": "yuudachi",
	"version": "3.0.0",
	"description": "",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --noEmit",
		"build:esm": "swc ./src --out-dir ./dist --strip-leading-paths",
		"build:copy-locales": "cpy locales/ dist/",
		"build": "pnpm --filter @yuudachi/framework build && pnpm run build:clean && pnpm run build:check && pnpm run build:esm && pnpm run build:copy-locales",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext ts --format=pretty",
		"format": "prettier --write . && cross-env TIMING=1 eslint src --ext ts --fix --format=pretty",
		"fmt": "pnpm run format",
		"start": "env-cmd node --enable-source-maps dist/index.js",
		"start:dev": "npm run build && npm run dev",
		"dev": "env-cmd node --enable-source-maps dist/index.js",
		"deploy:commands": "env-cmd node --enable-source-maps dist/deploy.js",
		"migrate": "env-cmd ley"
	},
	"type": "module",
	"directories": {
		"lib": "src"
	},
	"keywords": [],
	"author": "iCrawl <icrawltogo@gmail.com>",
	"license": "AGPL-3.0",
	"private": true,
	"dependencies": {
		"@aero/sanitizer": "^1.3.2",
		"@discordjs/rest": "^2.6.0",
		"@fastify/helmet": "^13.0.2",
		"@fastify/jwt": "^10.0.0",
		"@fastify/sensible": "^6.0.4",
		"@naval-base/ms": "^3.1.0",
		"@skyra/i18next-backend": "^2.0.6",
		"@swc/helpers": "^0.5.17",
		"@yuudachi/framework": "workspace:^",
		"bufferutil": "^4.0.9",
		"bullmq": "^5.65.1",
		"common-tags": "^1.8.2",
		"dayjs": "^1.11.19",
		"diff": "^8.0.2",
		"discord-api-types": "0.38.36",
		"discord.js": "^14.25.1",
		"env-cmd": "^11.0.0",
		"fastest-levenshtein": "^1.0.16",
		"fastify": "^5.6.2",
		"i18next": "^25.7.1",
		"ioredis": "^5.8.2",
		"kleur": "^4.1.5",
		"nanoid": "^5.1.6",
		"postgres": "^3.4.7",
		"prom-client": "^15.1.3",
		"re2": "^1.22.3",
		"readdirp": "^5.0.0",
		"redis": "^5.10.0",
		"reflect-metadata": "^0.2.2",
		"tsyringe": "4.8.0",
		"undici": "^7.16.0",
		"utf-8-validate": "^6.0.5",
		"ws": "^8.18.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.7.9",
		"@swc/core": "^1.15.3",
		"@types/common-tags": "^1.8.4",
		"@types/diff": "^8.0.0",
		"@types/node": "^24.10.1",
		"@types/ws": "^8.17.1",
		"cpy-cli": "^6.0.0",
		"cross-env": "^10.1.0",
		"del-cli": "^7.0.0",
		"eslint": "^9.39.0",
		"eslint-config-neon": "^0.2.9",
		"eslint-formatter-pretty": "^7.0.0",
		"ley": "^0.8.1",
		"prettier": "^3.7.4",
		"type-fest": "^5.3.0",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=24.0.0"
	}
}
