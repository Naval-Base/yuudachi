{
	"name": "yuudachi",
	"version": "3.0.0",
	"description": "",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --noEmit",
		"build:esm": "swc ./src --out-dir ./dist",
		"build:copy-locales": "cpy locales/ dist/",
		"build": "yarn workspace @yuudachi/framework run build && yarn build:clean && yarn build:check && yarn build:esm && yarn build:copy-locales",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext ts --format=pretty",
		"format": "prettier --write . && cross-env TIMING=1 eslint src --ext ts --fix --format=pretty",
		"fmt": "yarn format",
		"start": "env-cmd node --enable-source-maps dist/index.js",
		"start:dev": "npm run build && npm run dev",
		"dev": "env-cmd node --enable-source-maps dist/index.js",
		"deploy:commands": "env-cmd node --enable-source-maps dist/deploy.js",
		"migrate": "env-cmd ley"
	},
	"type": "module",
	"directories": {
		"lib": "src"
	},
	"keywords": [],
	"author": "iCrawl <icrawltogo@gmail.com>",
	"license": "AGPL-3.0",
	"private": true,
	"dependencies": {
		"@aero/sanitizer": "^1.3.2",
		"@discordjs/rest": "^2.2.0",
		"@fastify/helmet": "^11.1.1",
		"@fastify/jwt": "^7.2.3",
		"@fastify/sensible": "^5.5.0",
		"@naval-base/ms": "^3.1.0",
		"@skyra/i18next-backend": "^2.0.4",
		"@swc/helpers": "^0.5.3",
		"@yuudachi/framework": "workspace:^",
		"bufferutil": "^4.0.8",
		"bullmq": "^3.15.8",
		"common-tags": "^1.8.2",
		"dayjs": "^1.11.10",
		"diff": "^5.1.0",
		"discord-api-types": "0.37.55",
		"discord.js": "^14.14.1",
		"env-cmd": "^10.1.0",
		"fastest-levenshtein": "^1.0.16",
		"fastify": "^4.24.3",
		"i18next": "^23.7.6",
		"ioredis": "^5.3.2",
		"kleur": "^4.1.5",
		"nanoid": "^5.0.3",
		"postgres": "^3.4.3",
		"prom-client": "^15.0.0",
		"re2": "^1.20.8",
		"readdirp": "^3.6.0",
		"redis": "^4.6.10",
		"reflect-metadata": "^0.1.13",
		"tsyringe": "^4.8.0",
		"undici": "^5.27.2",
		"utf-8-validate": "^6.0.3",
		"ws": "^8.14.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.63",
		"@swc/core": "^1.3.96",
		"@types/common-tags": "^1.8.4",
		"@types/diff": "^5.0.8",
		"@types/node": "^18.18.10",
		"@types/ws": "^8.5.9",
		"cpy-cli": "^5.0.0",
		"cross-env": "^7.0.3",
		"del-cli": "^5.1.0",
		"eslint": "^8.54.0",
		"eslint-config-neon": "^0.1.57",
		"eslint-formatter-pretty": "^5.0.0",
		"ley": "^0.8.1",
		"prettier": "^3.1.0",
		"type-fest": "^4.8.1",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}
