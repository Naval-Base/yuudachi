{
	"name": "yuudachi",
	"version": "3.0.0",
	"private": true,
	"description": "",
	"keywords": [],
	"license": "AGPL-3.0",
	"author": "iCrawl <icrawltogo@gmail.com>",
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist",
		"linkshorteners.json",
		"locales",
		"migrations"
	],
	"type": "module",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsgo --noEmit",
		"build:esm": "swc ./src --out-dir ./dist --strip-leading-paths",
		"build:copy-locales": "cpy locales/ dist/",
		"build": "pnpm --filter @yuudachi/framework build && pnpm run build:clean && pnpm run build:esm && pnpm run build:copy-locales",
		"lint": "pnpm run build:check && oxfmt --check && oxlint --tsconfig=tsconfig.eslint.json --type-aware src",
		"format": "pnpm run build:check && oxfmt && oxlint --tsconfig=tsconfig.eslint.json --type-aware src --fix --fix-suggestions",
		"fmt": "pnpm run format",
		"start": "env-cmd --silent node --enable-source-maps dist/index.js",
		"start:dev": "npm run build && npm run dev",
		"dev": "env-cmd --silent node --enable-source-maps dist/index.js",
		"deploy:commands": "env-cmd --silent node --enable-source-maps dist/deploy.js",
		"migrate": "env-cmd --silent ley"
	},
	"dependencies": {
		"@aero/sanitizer": "^1.3.2",
		"@discordjs/rest": "^2.6.0",
		"@fastify/helmet": "^13.0.2",
		"@fastify/jwt": "^10.0.0",
		"@fastify/sensible": "^6.0.4",
		"@naval-base/ms": "^3.1.0",
		"@needle-di/core": "^1.1.0",
		"@skyra/i18next-backend": "^2.0.6",
		"@swc/helpers": "^0.5.18",
		"@yuudachi/framework": "workspace:^",
		"bufferutil": "^4.1.0",
		"bullmq": "^5.66.4",
		"common-tags": "^1.8.2",
		"dayjs": "^1.11.19",
		"diff": "^8.0.2",
		"discord-api-types": "0.38.37",
		"discord.js": "^14.25.1",
		"env-cmd": "^11.0.0",
		"fastest-levenshtein": "^1.0.16",
		"fastify": "^5.6.2",
		"i18next": "^25.7.4",
		"ioredis": "^5.9.1",
		"kleur": "^4.1.5",
		"nanoid": "^5.1.6",
		"postgres": "^3.4.8",
		"prom-client": "^15.1.3",
		"re2": "^1.23.0",
		"readdirp": "^5.0.0",
		"redis": "^5.10.0",
		"undici": "^7.18.2",
		"utf-8-validate": "^6.0.6",
		"ws": "^8.19.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.7.9",
		"@swc/core": "^1.15.8",
		"@types/common-tags": "^1.8.4",
		"@types/diff": "^8.0.0",
		"@types/node": "^25.0.5",
		"@types/ws": "^8.17.1",
		"@typescript/native-preview": "7.0.0-dev.20260109.1",
		"cpy-cli": "^6.0.0",
		"cross-env": "^10.1.0",
		"del-cli": "^7.0.0",
		"eslint": "^9.39.2",
		"eslint-config-neon": "^0.3.2",
		"eslint-formatter-compact": "^9.0.1",
		"eslint-formatter-pretty": "^7.0.0",
		"ley": "^0.8.1",
		"oxfmt": "^0.23.0",
		"oxlint": "^1.38.0",
		"oxlint-tsgolint": "^0.11.0",
		"prettier": "^3.7.4",
		"type-fest": "^5.3.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"engines": {
		"node": ">=24.0.0"
	}
}
