{
	"name": "@yuudachi/website",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"test": "vitest run",
		"build": "pnpm run build:css && pnpm run build:next",
		"build:check": "tsc --noEmit",
		"build:next": "next build",
		"build:css": "pnpm run generate:css",
		"dev": "concurrently 'pnpm run dev:css' 'pnpm run dev:next'",
		"dev:next": "next dev -p 8001",
		"dev:css": "pnpm run generate:css --watch",
		"generate:css": "unocss 'src/**/*.tsx' --out-file ./src/styles/unocss.css",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext mjs,js,ts,tsx --format=pretty",
		"format": "prettier --write . && cross-env TIMING=1 eslint src --ext mjs,js,ts,tsx --fix --format=pretty",
		"fmt": "pnpm run format"
	},
	"type": "module",
	"directories": {
		"lib": "src"
	},
	"keywords": [],
	"author": "iCrawl <icrawltogo@gmail.com>",
	"license": "AGPL-3.0",
	"private": true,
	"dependencies": {
		"@lukeed/ms": "^2.0.2",
		"@tanstack/react-table": "^8.21.3",
		"next": "^14.0.3",
		"next-themes": "^0.4.6",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"sharp": "^0.34.5"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^14.0.3",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@unocss/cli": "^0.57.7",
		"@unocss/reset": "^0.57.7",
		"@vercel/node": "^5.5.15",
		"@vitejs/plugin-react": "^5.1.1",
		"@vitest/coverage-v8": "^4.0.15",
		"concurrently": "^9.2.1",
		"cross-env": "^10.1.0",
		"eslint": "^9.39.1",
		"eslint-config-neon": "^0.2.9",
		"eslint-formatter-pretty": "^7.0.0",
		"eslint-plugin-react-compiler": "19.1.0-rc.2",
		"happy-dom": "^20.0.11",
		"prettier": "^3.7.4",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"typescript": "^5.9.3",
		"unocss": "^0.57.7",
		"vitest": "^4.0.15"
	},
	"engines": {
		"node": ">=24.0.0"
	}
}
