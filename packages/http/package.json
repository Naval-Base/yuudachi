{
	"name": "@yuudachi/http",
	"version": "0.1.5",
	"description": "",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --emitDeclarationOnly",
		"build:esm": "swc ./src --out-dir ./dist --strip-leading-paths",
		"build": "pnpm run build:clean && pnpm run build:check && pnpm run build:esm",
		"example": "pnpm --filter @yuudachi/framework build && pnpm run build && env-cmd tsx ./example/index.ts",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext ts --format=pretty",
		"format": "prettier --write . && cross-env TIMING=1 eslint src --ext ts --fix --format=pretty",
		"fmt": "pnpm run format",
		"prepack": "pnpm run build && pnpm run lint"
	},
	"type": "module",
	"module": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./types": {
			"types": "./dist/types/index.d.ts"
		}
	},
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"keywords": [],
	"author": "iCrawl <icrawltogo@gmail.com>",
	"license": "AGPL-3.0",
	"dependencies": {
		"@discordjs/core": "^2.4.0",
		"@fastify/helmet": "^13.0.2",
		"@fastify/sensible": "^6.0.4",
		"@swc/helpers": "^0.5.17",
		"discord-api-types": "0.38.36",
		"discord-verify": "^1.2.0",
		"fastify": "^5.6.2",
		"fastify-raw-body": "^5.0.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.7.9",
		"@swc/core": "^1.15.4",
		"@types/node": "^25.0.2",
		"@yuudachi/framework": "workspace:^",
		"cross-env": "^10.1.0",
		"del-cli": "^7.0.0",
		"env-cmd": "^11.0.0",
		"eslint": "^9.39.2",
		"eslint-config-neon": "^0.2.9",
		"eslint-formatter-pretty": "^7.0.0",
		"prettier": "^3.7.4",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=24.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
