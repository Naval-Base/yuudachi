		"@types/ws": "^8.5.3",
		"cpy-cli": "^4.2.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.25.0",
		"eslint-config-neon": "^0.1.35",
		"ley": "^0.7.1",
		"prettier": "^2.7.1",
		"type-fest": "^3.0.0",
		"typescript": "^4.8.4"
	},
	"engines": {
		"node": ">=16.0.0"
	}
