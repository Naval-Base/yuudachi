{
	"name": "yuudachi",
	"version": "3.0.0",
	"description": "",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --noEmit",
		"build:esm": "swc ./src --out-dir ./dist",
		"build:copy-locales": "cpy locales/ dist/",
		"build": "yarn workspace @yuudachi/framework run build && yarn build:clean && yarn build:check && yarn build:esm && yarn build:copy-locales",
		"lint": "prettier --check . && eslint src --ext ts",
		"format": "prettier --write . && eslint src --ext ts --fix",
		"fmt": "yarn format",
		"start": "env-cmd node --enable-source-maps dist/index.js",
		"start:dev": "npm run build && npm run dev",
		"dev": "env-cmd node --enable-source-maps dist/index.js",
		"deploy:commands": "env-cmd node --enable-source-maps dist/deploy.js",
		"migrate": "env-cmd ley"
	},
	"type": "module",
	"directories": {
		"lib": "src"
	},
	"keywords": [],
	"author": "iCrawl <icrawltogo@gmail.com>",
	"license": "AGPL-3.0",
	"private": true,
	"dependencies": {
		"@aero/sanitizer": "^1.2.0",
		"@discordjs/rest": "^1.2.0",
		"@naval-base/ms": "^3.1.0",
		"@skyra/i18next-backend": "^1.1.0",
		"@swc/helpers": "^0.4.11",
		"@yuudachi/framework": "workspace:^",
		"bufferutil": "^4.0.6",
		"bullmq": "^2.1.3",
		"common-tags": "^1.8.2",
		"dayjs": "^1.11.5",
		"diff": "^5.1.0",
		"discord-api-types": "^0.37.12",
		"discord.js": "^14.5.0",
		"env-cmd": "^10.1.0",
		"fastest-levenshtein": "^1.0.16",
		"i18next": "^21.10.0",
		"ioredis": "^5.2.3",
		"kleur": "^4.1.5",
		"nanoid": "^4.0.0",
		"postgres": "^3.3.1",
		"re2": "^1.17.7",
		"readdirp": "^3.6.0",
		"redis": "^4.3.1",
		"reflect-metadata": "^0.1.13",
		"tsyringe": "^4.7.0",
		"undici": "^5.11.0",
		"utf-8-validate": "^5.0.9",
