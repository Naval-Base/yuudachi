{
	"name": "@yuudachi/framework",
	"version": "0.1.4",
	"description": "",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --emitDeclarationOnly",
		"build:esm": "swc ./src --out-dir ./dist",
		"build": "yarn build:clean && yarn build:check && yarn build:esm",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext ts --format=pretty",
		"format": "prettier --write . && cross-env TIMING=1 eslint src --ext ts --fix --format=pretty",
		"fmt": "yarn format",
		"prepack": "yarn build && yarn lint"
	},
	"type": "module",
	"module": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./types": {
			"types": "./dist/types/index.d.ts"
		}
	},
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"keywords": [],
	"author": "iCrawl <icrawltogo@gmail.com>",
	"license": "AGPL-3.0",
	"dependencies": {
		"@swc/helpers": "^0.4.14",
		"pino": "^8.8.0",
		"readdirp": "^3.6.0",
		"reflect-metadata": "^0.1.13",
		"tsyringe": "^4.7.0",
		"type-fest": "^3.4.0"
	},
	"devDependencies": {
		"@discordjs/rest": "^1.4.0",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.23",
		"@types/node": "^18.11.15",
		"cross-env": "^7.0.3",
		"del-cli": "^5.0.0",
		"discord-api-types": "^0.37.20",
		"discord.js": "^14.7.1",
		"eslint": "^8.29.0",
		"eslint-config-neon": "^0.1.40",
		"eslint-formatter-pretty": "^4.1.0",
		"ioredis": "^5.2.4",
		"postgres": "^3.3.2",
		"prettier": "^2.8.1",
		"typescript": "^4.9.4"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
