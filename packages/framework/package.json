{
	"name": "@yuudachi/framework",
	"version": "0.3.5",
	"description": "",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --emitDeclarationOnly",
		"build:esm": "swc ./src --out-dir ./dist",
		"build": "pnpm run build:clean && pnpm run build:check && pnpm run build:esm",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext ts --format=pretty",
		"format": "prettier --write . && cross-env TIMING=1 eslint src --ext ts --fix --format=pretty",
		"fmt": "pnpm run format",
		"prepack": "pnpm run build && pnpm run lint"
	},
	"type": "module",
	"module": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./types": {
			"types": "./dist/types/index.d.ts"
		}
	},
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"keywords": [],
	"author": "iCrawl <icrawltogo@gmail.com>",
	"license": "AGPL-3.0",
	"dependencies": {
		"@discordjs/core": "^2.4.0",
		"@discordjs/rest": "^2.6.0",
		"@swc/helpers": "^0.5.17",
		"discord-api-types": "0.38.36",
		"discord.js": "^14.25.1",
		"pino": "^10.1.0",
		"readdirp": "^5.0.0",
		"reflect-metadata": "^0.2.2",
		"tsyringe": "^4.10.0",
		"type-fest": "^5.3.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.7.9",
		"@swc/core": "^1.15.3",
		"@types/node": "^24.10.1",
		"cross-env": "^10.1.0",
		"del-cli": "^7.0.0",
		"eslint": "^9.39.1",
		"eslint-config-neon": "^0.2.9",
		"eslint-formatter-pretty": "^7.0.0",
		"ioredis": "^5.8.2",
		"postgres": "^3.4.7",
		"prettier": "^3.7.4",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=24.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
